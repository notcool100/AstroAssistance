{
  "name": "astro-assistance-backend",
  "version": "1.0.0",
  "description": "Backend for AstroAssistance - Self-Learning AI Productivity Assistant",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
